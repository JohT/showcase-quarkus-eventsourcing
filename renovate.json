{
  "extends": [
    "config:base",
    "schedule:monthly",
    ":combinePatchMinorReleases",
    ":ignoreUnstable",
    ":automergeDigest",
    ":automergePatch",
    ":automergeMinor",
    ":automergeRequireAllStatusChecks",
    "group:recommended"
  ],
   "packageRules": [
    {
      "matchPackagePrefixes": [
        "io.quarkus:"
      ],
      "groupName": "Quarkus"
    },
    {
      "matchPackagePrefixes": [
      	"org.axonframework"
      ],
      "enabled": false
    }
  ]
  
}
